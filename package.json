{
  "name": "vasjs",
  "version": "1.2.0",
  "description": "A independent component for simplifying wave-like chart building.",
  "main": "dist/vas.min.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/vas.js",
  "jsdelivr": "dist/vas.js",
  "repository": "git@github.com:lbwa/vas.js.git",
  "author": "Bowen <asyncmode@gmail.com>",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/lbwa/vas.js#readme",
  "files": [
    "dist",
    "src"
  ],
  "bugs": {
    "url": "https://github.com/lbwa/vas.js/issues"
  },
  "scripts": {
    "clean": "rm -vrf dist/*",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config public/webpack.config.js",
    "build": "npm run clean && npm run build:prod && npm run build:dev",
    "build:prod": "cross-env TARGET=production rollup --config config/rollup.config.js",
    "build:dev": "cross-env TARGET=development rollup --config config/rollup.config.js"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "html-webpack-plugin": "^3.2.0",
    "rollup": "^1.16.7",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript2": "^0.21.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpackbar": "^3.2.0"
  },
  "keywords": [
    "canvas",
    "wave",
    "animation",
    "requestAnimationFrame",
    "wave-like",
    "flow",
    "flowing",
    "chart",
    "typescript",
    "util",
    "component",
    "fluid"
  ],
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "none"
  }
}
