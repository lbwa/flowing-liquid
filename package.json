{
  "name": "flowing-liquid",
  "version": "0.3.0",
  "description": "A component for building flowing chart",
  "main": "dist/flowing-liquid.min.js",
  "repository": "git@github.com:lbw/flowing-liquid.git",
  "bugs": {
    "url": "https://github.com/lbwa/flowing-liquid/issues"
  },
  "homepage": "https://github.com/lbwa/flowing-liquid#readme",
  "files": [
    "dist"
  ],
  "author": "Bowen <devto@outlook.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build:dev": "rollup -c config/rollup.config.js --environment TARGET:development",
    "build": "rollup -c config/rollup.config.js --environment TARGET:production",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config sample/webpack.config.js --progress",
    "server": "yarn build:dev && yarn dev",
    "docs": "rm -vrf docs/* && cross-env NODE_ENV=production webpack --config sample/webpack.config.js --progress",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "conventional-changelog-cli": "^2.0.11",
    "cross-env": "^5.2.0",
    "html-webpack-plugin": "^3.2.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^3.0.0",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpackbar": "^2.6.3"
  },
  "dependencies": {}
}
